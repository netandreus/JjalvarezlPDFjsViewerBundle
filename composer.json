{
    "name": "netandreus/pdfjs-viewer-bundle",
    "type": "symfony-bundle",
    "keywords": ["pdfjs", "Symfony", "Symfony4", "pdf viewer"],
    "description": "This bundle provides an integration of pdf.js from mozilla with Symfony 4 framework and customizable parameters on server side. Fix for Symfony 4.4+",
    "license": "MIT",
    "authors": [
        {
            "name": "jjalvarezl",
            "email": "jhon.jairo.alvarez.londono@gmail.com"
        },
        {
            "name": "Andrey Tokarchuk",
            "email": "netandreus@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.2"
    },
    "autoload": {
        "psr-0": { "jjalvarezl\\PDFjsViewerBundle" : "" }
    },
    "target-dir" : "jjalvarezl/PDFjsViewerBundle",
    "minimum-stability": "dev",
    "extra" : {
        "branch-alias" : {
            "dev-master" : "4.0-dev"
        }
    }
}
